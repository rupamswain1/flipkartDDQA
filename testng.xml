<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="flipkart QA" parallel="tests" thread-count="2" >
	
	<listeners>
	<listener class-name="com.flipkart.qa.Listner.Listner"></listener>
	</listeners>
	<test name="Validate Broken Link in FF" verbose="2">
	<parameter name="browser" value="chrome"/>
	<parameter name="headless" value="yes"/>
	<parameter name="imageDisable" value="yes"/>
		<classes>
			<class name="com.test.flipkart.BrokenLinkTest"></class>
		
		</classes>
	</test>
	
	
	
</suite>
